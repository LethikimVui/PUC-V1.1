    <main>
        <header>
            <div class="px-3 py-0 bg-success text-white">
                <div class="container">
                    <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
                        <a href="/" class="d-flex align-items-center my-2 my-lg-0 me-lg-auto text-white text-decoration-none">
                            <svg class="bi me-2" width="40" height="25" role="img" aria-label="Bootstrap"></svg>
                        </a>

                        <ul class="nav col-12 col-lg-auto my-2 justify-content-center my-md-0 text-small">

                            <li>
                                <a href="#" class="nav-link text-white">
                                    <svg class="bi d-block mx-auto mb-1" width="24" height="24"><use xlink:href="#speedometer2"></use></svg>
                                    Dashboard
                                </a>
                            </li>
                            <li>
                                <a href="#" class="nav-link text-white">
                                    <svg class="bi d-block mx-auto mb-1" width="24" height="24"><use xlink:href="#table"></use></svg>
                                    Maintenance
                                </a>
                            </li>

                            <li class="dropdown">
                                <a class="nav-link text-white" data-toggle="dropdown">
                                    <svg class="bi d-block mx-auto mb-1" width="24" height="24"><use xlink:href="#grid"></use></svg>
                                    Configuration
                                </a>
                                <div class="dropdown-menu">
                                    <a class="dropdown-item" asp-action="Add" asp-controller="Main">Add Machine / Fixture</a>
                                    <a class="dropdown-item" asp-action="Remove" asp-controller="Main">Remove Machine / Fixture</a>
                                    <a class="dropdown-item" asp-action="Add" asp-controller="Detail">Add Detail Part</a>
                                    <a class="dropdown-item" asp-action="Add" asp-controller="Common">Add Reason</a>
                                </div>

                            </li>

                            <li>
                                <a href="#" class="nav-link text-white">
                                    <svg class="bi d-block mx-auto mb-1" width="24" height="24"><use xlink:href="#people-circle"></use></svg>
                                    Administration
                                </a>
                            </li>
                            <li>
                                @if (User.Identity.IsAuthenticated)
                                {
                                    <p id="userinfo" class="nav-link text-white" data-user="@User.GetSpecificClaim("Ntlogin")">
                                        Hi @User.GetSpecificClaim("UserName") (@User.GetSpecificClaim("Ntlogin"))
                                        <a asp-action="Logout" asp-controller="Account" class="nav-link text-white">
                                            <i class="icon-key"></i> Log Out
                                        </a>
                                    </p>

                                }
                                else
                                {
                                    <a asp-action="Login" asp-controller="Account" class="nav-link text-white">
                                        <svg class="bi d-block mx-auto mb-1" width="24" height="24"></svg>
                                        Login
                                    </a>
                                }

                            </li>
                        </ul>
                    </div>
                </div>
            </div>

        </header>
        @RenderBody()
        <div class="scroll-to-top">
            <i class="icon-arrow-up"></i>
        </div>
